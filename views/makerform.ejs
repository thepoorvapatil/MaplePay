<div class="container">
      <h4>Payment Request ID: #8362AFD</h4>
      <form action="/makerform" method="POST">
        <div class="form-row">
          <div class="form-group col-md-4">
            <label for="pymnt">Method of Payment:</label>
            <input type="text" class="form-control" value="SWIFT" disabled>
          </div>
          <div class="form-group col-md-4">
            <label for="orderof">Order of:</label>
            <input type="text" class="form-control" id="orderof" placeholder="">
          </div>
          <div class="form-group col-md-4">
            <label for="issuedw">Order issued with:</label>
            <input type="text" class="form-control" id="issuedw" placeholder="">
          </div>
        </div>
        <div class="form-row">
          <div class="form-group col-md-6">
            <label for="callback">Callback Performed:</label>
            <input type="date" class="form-control" id="callback" placeholder="">
          </div>
          <div class="form-group col-md-6">
            <label for="pinstruct">Name of person(s) instructing:</label>
            <input type="text" class="form-control" id="pinstruct" placeholder="">
          </div>
        </div>
        <div class="form-row">
          <div class="form-group col-md-6">
            <label for="callbackwho">Who was called back:</label>
            <input type="text" class="form-control" id="callbackwho" placeholder="">
