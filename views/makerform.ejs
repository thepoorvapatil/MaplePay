                <div class="panel-body">
                  <label for="debitacc">Debit Account:</label>
                  <input type="text" class="form-control" id="debitacc" value="006-091-993" disabled>
                  <div class="panel-group">
                    <div class="panel panel-default">
                      <div class="panel-heading"><strong>Payment</strong></div>
                      <div class="panel-body">
                        <div class="row">
                          <div class="col-sm-6">
                            <label for="currency">Currency:</label>
                            <input type="text" class="form-control" id="currency" value="USD" disabled>
                          </div>
                            <div class="col-sm-6">
                                <label for="currency">Amount:</label>
                              <input type="text" class="form-control" id="amt" value="10,000" disabled>
                            </div>
                        </div>
                        <label for="dateofdebt">Value date of debt:</label>
                        <input type="text" class="form-control" id="dateofdebt" value="16/05/2019" disabled>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="form-group col-md-6">
            <div class="panel-group">
              <div class="panel panel-default">
                <div class="panel-heading"><strong>Beneficiary:</strong> Customer/Bank</div>
                <div class="panel-body">
                  <label for="accno">Account No:</label>
                  <input type="text" class="form-control" id="accno" value="1-6082-018.F" disabled>
                  <h1></h1>
                  <h1></h1>
                  <label for="name">Name:</label>
                  <input type="text" class="form-control" id="name" value="Phalange Family Trust" disabled>
                  <h1></h1>
                  <h1></h1>
                  <label for="benadd">Address:</label>
                  <textarea class="form-control rounded-0" id="benadd" rows="2" disabled>Singapore</textarea>
                </div>
              </div>
            </div>
          </div>
        </div>
        
        <div class="form-row">
          <div class="form-group col-md-6">
            <div class="panel-group">
              <div class="panel panel-default">
                <div class="panel-body">
                  <label for="paypurpose">Payment Purpose:</label>
                  <textarea class="form-control rounded-0" id="paypurpose" rows="3" disabled>Personal expenses</textarea>
                  <label for="msgOp">Message to Operations</label>
                  <textarea class="form-control rounded-0" id="msgOp" rows="4"></textarea>
                </div>
              </div>
            </div>
          </div>
          <div class="form-group col-md-6">
              <div class="panel-group">
                <div class="panel panel-default">
                  <div class="panel-heading"><strong>Bank of the Beneficiary</strong></div>
                  <div class="panel-body">
                    <label for="bankcode">Bankcode:</label>
                    <input type="text" class="form-control" id="bankcode" value="543" disabled>
                    <label for="benbankname">Name:</label>
                    <input type="text" class="form-control" id="benbankname" value="Geller Bank" disabled>
                    <label for="benbankadd">Address:</label>
                    <textarea class="form-control rounded-0" id="benbankadd" rows="2" disabled>25/F Trade Centre, Singapore</textarea>
                  </div>
                </div>
              </div>
          </div>
        </div>
        <a href="/makerworkdone" class="btn btn-primary">Submit</a>
